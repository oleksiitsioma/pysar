{
	"name": "pysar",
	"version": "1.0.0",
	"description": "Advanced plugin for form generation in WordPress.",
	"main": "index.js",
	"scripts": {
		"webpack": "webpack watch --mode=development",
		"dist": "webpack --mode=production",
		"build": "wp-scripts build",
		"lint": "wp-scripts lint-js",
		"format": "wp-scripts format",
		"start:input": "wp-scripts start blocks/input/src/index.js --output-path=blocks/input/build/",
		"build:input": "wp-scripts build blocks/input/src/index.js --output-path=blocks/input/build/",
		"start:textarea": "wp-scripts start blocks/textarea/src/index.js --output-path=blocks/textarea/build/",
		"build:textarea": "wp-scripts build blocks/textarea/src/index.js --output-path=blocks/textarea/build/",
		"start:submit": "wp-scripts start blocks/submit/src/index.js --output-path=blocks/submit/build/",
		"build:submit": "wp-scripts build blocks/submit/src/index.js --output-path=blocks/submit/build/"
	},
	"author": "Oleksii Tsioma <oleksiitsioma@gmail.com> (https://www.linkedin.com/in/oleksiitsioma/)",
	"license": "ISC",
	"devDependencies": {
		"@wordpress/scripts": "^25.3.0",
		"mini-css-extract-plugin": "^2.7.2",
		"sass": "^1.58.0",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"dependencies": {
		"@wordpress/block-editor": "^11.3.0",
		"@wordpress/blocks": "^12.3.0"
	}
}
